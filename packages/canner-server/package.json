{
  "name": "@canner/server",
  "version": "0.0.5",
  "description": "Build CMS and GraphQL server with CannerCMS and GQLify",
  "author": "Canner",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "web:statics:build": "NODE_ENV=production ts-node src/web/client/bin/build.ts",
    "web:statics:serve": "NODE_ENV=production ts-node src/web/client/bin/serve.ts",
    "web:dataSources": "NODE_ENV=production ts-node src/web/client/bin/dataSources.ts",
    "web:server": "NODE_ENV=production ts-node src/web/server/bin/www.ts",
    "web:start": "npm run web:statics:build && npm run web:dataSources && npm run web:server",
    "gqlify:server": "NODE_ENV=development ts-node src/gqlify/bin/www.ts",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha --timeout 10000 --compilers ts:ts-node/register --recursive --exit \"test/**/*.spec.ts\"",
    "clean": "rimraf lib dist",
    "prebuild": "npm run clean",
    "build": "tsc -p ./tsconfig.release.json --pretty && copyfiles -u 4 \"src/web/client/statics/**/*\" lib/web/client/statics && copyfiles -u 4 \"src/web/server/views/**/*\" lib/web/server/views",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-decorators": "7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0",
    "@babel/plugin-proposal-function-sent": "7.0.0",
    "@babel/plugin-proposal-json-strings": "7.0.0",
    "@babel/plugin-proposal-numeric-separator": "7.0.0",
    "@babel/plugin-proposal-throw-expressions": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-syntax-import-meta": "7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@canner/antd-components": "^0.5.28",
    "@canner/container": "^2.0.37",
    "@canner/history-router": "^2.0.11",
    "@canner/storage": "2.0.11",
    "@gqlify/canner-schema-to-gqlify-model": "1.0.4",
    "@gqlify/server": "1.1.4",
    "antd": "^3.11.5",
    "apollo-server": "^2.3.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "^1.9.1",
    "canner": "^2.0.45",
    "canner-schema-loader": "^2.0.37",
    "canner-script": "^2.0.36",
    "compression-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "ignore-loader": "^0.1.2",
    "koa": "^2.6.2",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.1.5",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.5",
    "lowdb": "^1.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "minimist": "^1.2.0",
    "pluralize": "^7.0.0",
    "pug": "^2.0.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.21.0",
    "styled-components": "^3.2.6",
    "tmp": "^0.0.33",
    "ts-import-plugin": "^1.5.5",
    "ts-loader": "^4.3.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.107",
    "@types/minimist": "^1.2.0",
    "@types/node": "^9.4.6",
    "@types/webpack": "^4.4.22",
    "@types/webpack-dev-server": "^3.1.1",
    "firebase-admin": "^6.4.0",
    "copyfiles": "^2.1.0"
  },
  "repository": "Canner/canner",
  "license": "MIT"
}
