{
  "name": "@canner/server",
  "version": "3.0.0-alpha.24",
  "description": "Build CMS and GraphQL server with CannerCMS and GQLify",
  "author": "Canner",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "gqlify:server": "NODE_ENV=development ts-node src/gqlify/bin/www.ts",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha --timeout 10000 --compilers ts:ts-node/register --recursive --exit \"test/**/*.spec.ts\"",
    "clean": "rimraf lib dist",
    "prebuild": "npm run clean",
    "build": "tsc -p ./tsconfig.release.json --pretty && copyfiles -u 4 \"src/web/server/views/**/*\" lib/web/server/views",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@canner/auth-server": "3.0.0-alpha.21",
    "@canner/cms-server": "3.0.0-alpha.24",
    "@canner/graphql-server": "3.0.0-alpha.21",
    "@canner/server-common": "3.0.0-alpha.21",
    "boom": "^7.3.0",
    "graphql-playground-html": "^1.6.11",
    "koa": "^2.6.2",
    "lodash": "^4.17.5",
    "openid-client": "^2.4.5"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.25",
    "@types/boom": "^7.2.1",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/koa": "^2.0.48",
    "@types/koa-mount": "^3.0.1",
    "@types/koa-router": "^7.0.37",
    "@types/koa-send": "^4.1.1",
    "@types/koa-static": "^4.0.0",
    "@types/lodash": "^4.14.107",
    "@types/minimist": "^1.2.0",
    "@types/node": "^9.4.6",
    "@types/webpack": "^4.4.22",
    "@types/webpack-dev-server": "^3.1.1",
    "copyfiles": "^2.1.0",
    "rimraf": "^2.5.4",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-react": "^3.6.0",
    "typescript": "^2.7.2"
  },
  "repository": "Canner/canner",
  "license": "MIT",
  "gitHead": "3cbbad04bd29c8d70a01144c3eb2c1e20a5eae8c"
}
