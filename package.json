{
  "name": "@canner/react-cms-core",
  "version": "1.0.0",
  "description": "the core-part of canner cms",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src test docs",
    "clean": "rimraf lib dist",
    "build:flow": "flow-copy-source -v -i '**/test/**' src lib",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build": "npm run build:commonjs && npm run build:flow",
    "prepublish": "npm run clean && npm run check:src && npm run build",
    "check:src": "npm run lint",
    "start": "./node_modules/.bin/webpack-dev-server --mode development",
    "postpublish": "npm run build:docs",
    "build:docs": "./node_modules/.bin/webpack ./docs/index.js --mode production --ouput ./docs/static/bundle.js",
    "validate": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Canner/react-cms-core.git"
  },
  "keywords": [
    "canner",
    "cms",
    "core"
  ],
  "author": "Canner",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Canner/react-cms-core/issues"
  },
  "homepage": "https://github.com/Canner/react-cms-core#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.18.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.66.0",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.10"
  },
  "dependencies": {
    "bson-objectid": "^1.2.2",
    "immutable": "^3.8.2",
    "invariant": "^2.2.3",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.1",
    "rxjs": "^5.5.6"
  }
}
